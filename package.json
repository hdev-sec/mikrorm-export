{
  "name": "mikrorm-export",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "initdb": "npx mikro-orm schema:fresh --run",
    "build": "rm -rf ./dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "createmigration": "npx mikro-orm migration:create --initial"
  },
  "author": "hdev-sec",
  "license": "MIT",
  "dependencies": {
    "@mikro-orm/core": "^5.7.12",
    "@mikro-orm/mysql": "^5.7.12",
    "dotenv": "^16.3.1"
  },
  "mikro-orm": {
    "configPaths": [
      "./mikro-orm.config.js"
    ]
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.7.12",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  }
}
